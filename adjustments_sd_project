student_full_name TEXT,
fees_name TEXT,
original_amount FLOAT,
adjustment_amount FLOAT,
new_amount FLOAT,
reason_for_adjustment TEXT,
adjusted_by TEXT
school_2 TEXT, 
student_name TEXT, 
sd_liability_balance TEXT, 
school_year_2 TEXT, 
school_id_2 INT, 
family_id_2 INT, 
indicator_id_2 INT, 
payment_amount TEXT, 
payment_method TEXT, 
payment_date TEXT, 
notes TEXT, 
account_holder_primary TEXT, 
account_holder_secondary TEXT, 
full_address TEXT, 
email_primary TEXT, 
email_secondary TEXT, 
phone_primary INT, 
phone_secondary INT, 
mobile_number INT, 
international_number INT
);


school AS school_2,
student_name, 
sd_liability_balance, 
school_year AS school_year_2,
school_id AS school_id_2,
family_id AS family_id_2,
indicator_id AS indicator_id_2,
payment_amount, 
