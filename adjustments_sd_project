SELECT *
FROM(
SELECT *
FROM (
SELECT DISTINCT family_id AS distinct_familY_id
FROM family_ids_for_project) AS family_ids_project2
JOIN adjustments_all
ON adjustments_all.family_id = family_ids_project2.distinct_family_id)
WHERE fees_name LIKE "%ATF - Account Credit Donated%"
OR fees_name LIKE "%ATF - SD Donated to ATF%"
OR fees_name LIKE "%ATF - Security Deposit Donated%"
